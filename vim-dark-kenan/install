#!/bin/bash 
docker pull kenandervisagic/vim-container:latest
wget 'https://github.com/kenandervisagic/vim-dark-mode/blob/main/vim-dark-kenan/start_container' -O /usr/local/bin/start_container && chmod +x /usr/local/bin/start_container
wget 'https://github.com/kenandervisagic/vim-dark-mode/blob/main/vim-dark-kenan/bin/fzy?inline=false-o' -O /usr/local/bin/fzy && chmod +x /usr/local/bin/fzy
